public class Main {
    public static void main(String[] args)
    {


        int a = 4;
        int b = 2;
        int sum =a + b;
        int dif =a - b;
        int mull =a * b;
        int div =a / b;
        int mod =a % b;

        System.out.println(sum);
        System.out.println(dif);
        System.out.println(mul);
        System.out.println(div);
        System.out.println(mod);

    }
}
